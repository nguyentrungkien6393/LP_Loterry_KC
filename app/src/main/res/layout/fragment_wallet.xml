<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.wallet.Fragment_Wallet">
    <TextView
        android:gravity="center"
        android:padding="10dp"
        android:text="Địa Chỉ Ví"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/mTV_wallet"
        android:background="@drawable/bogoc"
        android:gravity="center"
        android:padding="10dp"
        android:text="Địa Chỉ Ví"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:gravity="center"
        android:padding="10dp"
        android:text="Số Lượng LTR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/mTV_LTR_Result"
        android:background="@drawable/bogoc"
        android:gravity="center"
        android:padding="10dp"
        android:text="Địa Chỉ Ví"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:gravity="center"
        android:padding="10dp"
        android:text="Số Lượng ETH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/mTV_ETH_Result"
        android:background="@drawable/bogoc"
        android:gravity="center"
        android:padding="10dp"
        android:text="Địa Chỉ Ví"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>